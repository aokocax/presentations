.selider
    overflow hidden
    position relative
    .carouselContainer
        position absolute
        list-style none
        margin 0
        padding 0
        li
            float left
            position relative
            p
                background #000
                background rgba(0, 0, 0, 0.5) !important
                padding 5px 10px
                cursor pointer
                position absolute
                left 10px
                bottom 5px
                color #FFF
                font-family Helvetica

#sPaging
    position absolute
    ul
        height 18px
        margin 5px auto
    li
        display block
        height 10px
        width 10px
        background-color #b4b4b4
        border-radius 10px
        box-shadow 1px 1px 0 #fff
        float left
        margin-right 5px
    .on
        cursor pointer
        background-color #838383